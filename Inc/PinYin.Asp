    splStr = Split(getftext("1.txt"), "End If") 
    For i = 0 To UBound(splStr)
        s = splStr(i) 
        If InStr(s, "=") > 0 Then
            cn = Mid(s, InStr(s, """") + 1) 
            cn = Mid(cn, 1, InStr(cn, " ") - 3) 

            en = Mid(s, InStr(s, "=") + 1) 
            en = Mid(en, 3, InStr(en, vbCrLf) - 4) 
            c = "PY(" & i & ")=""" & cn & "_" & en & """" 
            Call rw(c) 
        End If 
    Next 
End Sub 

