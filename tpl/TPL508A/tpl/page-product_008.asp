rs.movenext:Next:End If:rs.close
%>


 
                    </ul>
                    <div class="xypg-pagination">
                        <div class="pagin-left">
                            <!-- 总计 -->
                            <a>共 <%=iCount%> 条记录</a> 
                            <%
 if page>1 then
                response.Write("<a href="""& pageUrl &""">首页</a>")
                response.Write("<a href="""& getUpDownPageUrl(pageUrl,page-1) &""">上一页</a>") 
            end if  
                            %>
                            <!-- 上一页 -->
                        </div>
                        <div class="pagin-mid">
                            <div class="pc-pagin-mid">
                                <!-- 数字 --> 
                                <%

            for i=page-2 to page+2
                if i>0 and i<=maxpage then
                    if i=page then
                        response.Write("<span id=""current"">"& i &"</span>")
                    else
                        response.Write("<a href="""& getUpDownPageUrl(pageUrl,i) &""">"& i &"</a>")
                    end if
                end if
            next
                                %>
                            </div>
                            <div class="mob-pagin-mid">
                                <!-- 当前页数  /  总页数 -->
                                <span><%=page%>/<%=maxpage%></span>
                            </div>
                        </div>
                        <div class="pagin-right">
              <% 
            if page<maxpage then
                response.Write("<a href="""& getUpDownPageUrl(pageUrl,page+1) &""">下一页</a>")
                response.Write("<a href="""& getUpDownPageUrl(pageUrl,maxpage) &""">末页</a>")
            end if 
        %> 
                        </div>
                        <div class="pagin-select">
                            <select onchange="window.location=this.options[this.selectedIndex].value">
                                <%
                                for i=1 to maxpage
                                %>
                                <option value="<%=getUpDownPageUrl(pageUrl,i)%>" <%=IIF(page=i,"selected","")%>>第<%=i%>页</option>
                                <%next%>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="page-mob-tool">
                <ul>
                    <li class="xymob-page-navbtn">
