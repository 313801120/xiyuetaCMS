}
.gsjj<%=cssName%> .fw_sp{
  display: flex;
  justify-content: center;
}
.gsjj<%=cssName%> .fw_sp>span{
  float: left;
  position: relative;
  padding: 0 20px;
  line-height: 40px;
  font-size: 30px;
  color: #333;
  font-weight: bold;
}
.gsjj<%=cssName%> .fw_sp>span>em{
  color: #0d62be;
  font-style: normal;
}
.gsjj<%=cssName%> .fw_sp>span::before{
  content: "";
  width: 304px;
  float: left;
  height: 1px;
  background-color: #0d62be;
  position: absolute;
  right: 100%;
  top: 50%;
  transform: translateY(-50%);
}
.gsjj<%=cssName%> .fw_sp>span::after{
  content: "";
  width: 304px;
  float: left;
  height: 1px;
  background-color: #0d62be;
  position: absolute;
  left: 100%;
  top: 50%;
  transform: translateY(-50%);
}
.gsjj<%=cssName%>{
  width: 100%;
  overflow: hidden;
  background: #0d62be;
  background-size: cover;
  padding: 60px 0 45px 0;
}
.gsjj<%=cssName%> .fw_p{
  color: #fff;
}
.gsjj<%=cssName%> .fw_sp>span,.gsjj<%=cssName%> .fw_sp>span>em{
  color: #fff;
}
.gsjj<%=cssName%> .fw_sp>span::before,.gsjj<%=cssName%> .fw_sp>span::after{
  background-color: #fff;
}
.gsjj<%=cssName%> .gsjj_contect{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  flex-direction: row-reverse;
  margin-top: 40px;
}
.gsjj<%=cssName%> .gsjj_img{
  width: calc(50% - 40px);
  float: right;
  border: 3px solid #fff;
  overflow: hidden;
  position: relative;
}
.gsjj<%=cssName%> .gsjj_left{
  width: 50%;
  float: left;
}
.gsjj<%=cssName%> .gsjj_p{
  display: block;
  overflow: hidden;
  line-height: 24px;
  font-size: 20px;
  color: #fff;
  font-weight: bold;
  margin-bottom: 30px;
}
.gsjj<%=cssName%> .gsjj_txt{
  display: block;
  overflow: hidden;
  line-height: 30px;
  height: 210px;
  font-size: 14px;
  color: #fff;
  text-overflow:ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 7;
  -webkit-box-orient: vertical;
}
.gsjj<%=cssName%> .gsjj_more{
  display: block;
  overflow: hidden;
  padding-top: 35px;
}
.gsjj<%=cssName%> .gsjj_more>a{
  display: block;
  overflow: hidden;
  width: 134px;
  height: 46px;
  border: 2px solid #fff;
  background-color: #fff;
  line-height: 42px;
  text-align: center;
  font-size: 14px;
  color: #0d62be;
}
.gsjj<%=cssName%> .gsjj_more>a:hover{
  background-color: #0d62be;
  color: #fff;
}
@media (max-width: 1220px){
.gsjj<%=cssName%>{
  padding: 40px 0;
}
.gsjj<%=cssName%> .gsjj_img{
  width: calc(50% - 30px);
}
.gsjj<%=cssName%> .gsjj_p{
  margin-bottom: 20px;
}
.gsjj<%=cssName%> .gsjj_more{
  padding-top: 20px;
}}
@media (max-width: 991px){
