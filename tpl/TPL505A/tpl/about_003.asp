<style>


.gsjj<%=cssName%> *{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;margin:0;padding:0}
.gsjj<%=cssName%> :after,.gsjj<%=cssName%> :before{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box}
.gsjj<%=cssName%> body{background-color:#fff;font:14px Arial,"Microsoft YaHei",SimHei,SimSun;overflow-x:hidden}
.gsjj<%=cssName%> button,.gsjj<%=cssName%> img,.gsjj<%=cssName%> input,.gsjj<%=cssName%> select,.gsjj<%=cssName%> textarea{border:none;outline:0}

.gsjj<%=cssName%> a{text-decoration: none;}
.gsjj<%=cssName%> a:hover;{text-decoration: none;}
.gsjj<%=cssName%> .img-cover{position:absolute;top:0;width:100%;height:100%;display:block;object-fit:cover;transition:all 1s;-webkit-transition:all 1s;-moz-transition:all 1s;-ms-transition:all 1s;-o-transition:all 1s}
.gsjj<%=cssName%> a{transition:all .4s;-webkit-transition:all .4s;-moz-transition:all .4s;-ms-transition:all .4s;-o-transition:all .4s}
.gsjj<%=cssName%> .container{max-width:1200px;margin:0 auto}
@media (max-width:767px){
.gsjj<%=cssName%> .hidden-xs{display:none!important}
.gsjj<%=cssName%> .container{max-width:calc(100% - 20px)!important}}
@media (min-width:768px) and (max-width:991px){
.gsjj<%=cssName%> .container{max-width:calc(100% - 20px)!important}}
@media (min-width:992px) and (max-width:1199px){
.gsjj<%=cssName%> .container{max-width:98%!important}}
.gsjj<%=cssName%> .fw_title{
  display: block;
  overflow: hidden;
  text-align: center;
}
.gsjj<%=cssName%> .fw_p{
  line-height: 30px;
  font-size: 24px;
  color: #999;
  font-weight: bold;
