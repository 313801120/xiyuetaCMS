}
.gsjj<%=cssName%> .gsjj_more{
  padding-top: 10px;
}}
@media (max-width: 767px){
.gsjj<%=cssName%> .fw_p{
  line-height: 20px;
  font-size: 14px;
}
.gsjj<%=cssName%> .fw_sp>span{
  padding: 0 10px;
  line-height: 30px;
  font-size: 20px;
}
.gsjj<%=cssName%>{
  padding: 10px 0;
}
.gsjj<%=cssName%> .gsjj_contect{
  margin-top: 10px;
}
.gsjj<%=cssName%> .gsjj_img{
  width: 100%;
}
.gsjj<%=cssName%> .gsjj_img::before{
  content: "";
  display: block;
  padding-top: 50%;
}
.gsjj<%=cssName%> .gsjj_left{
  width: 100%;
  margin-top: 10px;
}
.gsjj<%=cssName%> .gsjj_txt{
  line-height: 20px;
  height: auto;
  -webkit-line-clamp: initial;
}
.gsjj<%=cssName%> .gsjj_more>a{
  width: 104px;
  height: 34px;
  line-height: 30px;
}}

</style> 
